for printer in `lpstat -p | awk '{print $2}'`
do
lpadmin -x $printer
done

curl http://10.8.9.89/hp-color_laserjet_m651-ps.ppd.gz --output /Library/Printers/PPDs/Contents/Resources/hp-color_laserjet_m651-ps.ppd.gz

lpadmin -p "HPofficeJetM651" -L "2 FL" -E -v lpd://[HP_IP_1] -P /Library/Printers/PPDs/Contents/Resources/hp-color_laserjet_m651-ps.ppd.gz -o HPOptionDuplexer=True -o printer-is-shared=false -o cupsIPPSupplies=true -o auth-info-required=none -L 2FL -u allow:all -E

# Enable all printers
cupsenable HPofficeJetM651

exit 0
